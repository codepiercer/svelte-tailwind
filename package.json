{
  "name": "@codepiercer/svelte-tailwind",
  "version": "0.1.23",
  "scripts": {
    "dev": "vite dev",
    "build": "npm run lint && svelte-package",
    "publish": "npm run build && npm publish ./package --access public",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir=. . --check && eslint . && npm run check",
    "lint:fix": "prettier --plugin-search-dir=. . --write && eslint . --fix",
    "husky": "husky install && husky add .husky/pre-commit \" npm run lint\""
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "2.0.0",
    "@sveltejs/package": "1.0.2",
    "@typescript-eslint/eslint-plugin": "5.50.0",
    "@typescript-eslint/parser": "5.50.0",
    "@tanstack/svelte-query": "4.24.4",
    "svelte-french-toast": "1.0.3",
    "svelte-check": "3.0.3",
    "tslib": "2.5.0",
    "typescript": "4.9.5"
  },
  "peerDependencies": {
    "@sveltejs/kit": "1.5.0",
    "@tailwindcss/forms": "0.5.3",
    "@tailwindcss/typography": "0.5.9",
    "autoprefixer": "10.4.13",
    "eslint": "8.33.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-svelte3": "4.0.0",
    "flatpickr": "4.6.13",
    "husky": "8.0.3",
    "postcss": "8.4.21",
    "prettier": "2.8.3",
    "prettier-plugin-svelte": "2.9.0",
    "prettier-plugin-tailwindcss": "0.2.2",
    "svelte": "3.55.1",
    "svelte-forms-lib": "2.0.1",
    "tailwindcss": "3.2.4",
    "tailwind-merge": "1.9.1",
    "vite": "4.1.1",
    "yup": "0.32.11"
  },
  "type": "module"
}
